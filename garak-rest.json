{
  "rest.RestGenerator": {
    "name": "openai-proxy",
    "uri": "http://localhost:30080/v1/chat/completions",
    "method": "post",
    "headers": {},
    "req_template_json_object": {
      "model": "proxy:gpt-4o",
      "messages": [
        {
          "role": "user",
          "content": "$INPUT"
        }
      ],
      "format": "json",
      "stream": false
    },
    "response_json": true,
    "response_json_field": "$.choices[0].message.content",
    "request_timeout": 30
  }
}
